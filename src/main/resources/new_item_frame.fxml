<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<fx:root type="javafx.scene.layout.AnchorPane" fx:id="rootAnchorPane" prefHeight="316.0" prefWidth="600.0"
         xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="nameLabel" layoutX="40.0" layoutY="70.0" prefHeight="35.0" text="Name:">
            <font>
                <Font name="Arial Bold" size="16.0"/>
            </font>
        </Label>
        <TextField fx:id="nameTextField" layoutX="180.0" layoutY="70.0" prefHeight="35.0" prefWidth="380.0"
                   promptText="Prompt text">
            <font>
                <Font name="Arial Bold" size="16.0"/>
            </font>
        </TextField>
        <Label layoutX="40.0" layoutY="120.0" prefHeight="35.0" text="Description:">
            <font>
                <Font name="Arial Bold" size="16.0"/>
            </font>
        </Label>
        <TextArea fx:id="descriptionTextArea" layoutX="180.0" layoutY="125.0" prefHeight="130.0" prefWidth="380.0">
            <font>
                <Font name="Arial Bold" size="14.0"/>
            </font>
        </TextArea>
        <Label fx:id="messageLabel" layoutX="40.0" layoutY="20.0" prefHeight="35.0" text="Label">
            <font>
                <Font name="Arial Bold" size="16.0"/>
            </font>
        </Label>
        <Button fx:id="addButton" layoutX="480.0" layoutY="267.0" mnemonicParsing="false" prefHeight="35.0"
                prefWidth="80.0" text="Add" textAlignment="CENTER">
            <font>
                <Font name="Arial Bold" size="16.0"/>
            </font>
        </Button>
    </children>
</fx:root>
